{
  "name": "branchless-batch-compress",
  "version": "1.0.0",
  "description": "Allow to compile and compress branchless batch expression",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "start": "npm run watch",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack serve --mode=production --node-env=production",
    "format": "prettier --config .prettierrc.json src/**/*.ts --write",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "grammar:generate": "lezer-generator src/languages/batch/batch.grammar -o src/languages/batch/parser --typeScript && npm run format && npm run lint:fix"
  },
  "author": "Flammrock",
  "license": "MIT",
  "devDependencies": {
    "@lezer/common": "^1.2.1",
    "@lezer/generator": "^1.7.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/webpack-node-externals": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@webpack-cli/generators": "^3.0.7",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "json5": "^2.2.3",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.2",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@lezer/lr": "^1.4.0",
    "codemirror": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
